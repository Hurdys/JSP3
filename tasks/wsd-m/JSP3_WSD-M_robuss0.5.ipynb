    "            if res: return res[0].definition()\n",
    "    except: pass\n",
    "    return \"Def not found\"\n",
    "\n",
    "def get_czech_synonyms(synset):\n",
    "    \"\"\"Vr치t칤 캜esk치 synonyma pro dan칳 synset\"\"\"\n",
    "    lemmas = [w.lemma() for w in synset.words()]\n",
    "    return \", \".join(lemmas[:5]) # Max 4 synonyms\n",
    "\n",
